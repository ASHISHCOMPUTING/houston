{{> nav-secondary}}

<div class="row">
    <h2>Select an App:</h2>
    {{#each repos}}<a class="repo" href="/project/gh/{{full_name}}">
        <img class="app-icon" src="/images/application-default-icon.png"/>
        <div class="description">
            <p>{{name}}</p>
            <p class="subtext">{{full_name}}</p>
        </div>
    </a>{{/each}}
</div>
